{
	"name": "atelierjs",
	"version": "0.0.1",
	"description": "atelierJS is a tool for creating digital art",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/veit30/atelierJS.git"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"keywords": [
		"generative",
		"art",
		"plotter",
		"threejs",
		"creative"
	],
	"author": "veit30",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/veit30/atelierJS/issues"
	},
	"homepage": "https://github.com/veit30/atelierJS#readme",
	"dependencies": {
		"@supabase/auth-helpers-svelte": "^0.4.2",
		"@supabase/auth-helpers-sveltekit": "^0.6.1",
		"@supabase/supabase-js": "1.35.3",
		"@svuick/form": "^0.0.2",
		"@svuick/modal": "^0.0.5",
		"monaco-editor": "^0.33.0",
		"p5": "^1.4.1",
		"rollup": "^2.75.6",
		"source-map": "^0.7.4",
		"svelte-dnd-action": "^0.9.18"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.2",
		"tslib": "^2.4.0",
		"typescript": "^4.7.3"
	},
	"type": "module"
}

{
	"name": "atelierjs",
	"version": "0.0.1",
	"description": "atelierJS is a tool for creating digital art",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/veit30/atelierJS.git"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"keywords": [
		"generative",
		"art",
		"plotter",
		"threejs",
		"creative"
	],
	"author": "veit30",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/veit30/atelierJS/issues"
	},
	"homepage": "https://github.com/veit30/atelierJS#readme",
	"dependencies": {
		"@svuick/cookie": "^0.0.8",
		"@svuick/core": "^0.0.14",
		"@svuick/form": "^0.0.2",
		"@svuick/modal": "^0.0.5",
		"@svuick/supabase": "^0.0.24",
		"canvas": "^2.9.0",
		"monaco-editor": "^0.32.1",
		"p5": "^1.4.1",
		"rollup": "^2.67.3",
		"source-map": "^0.7.3"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.4.1",
		"@types/tailwindcss": "^3.0.8",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"autoprefixer": "^10.4.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"postcss": "^8.4.6",
		"postcss-load-config": "^3.1.3",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.5",
		"svelte-preprocess": "^4.10.3",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5"
	},
	"type": "module"
}
